{
  "name": "warframeLfg",
  "private": true,
  "version": "0.0.1",
  "description": "Warframe LFG",
  "main": "server.js",
  "scripts": {
    "lint": "eslint ./src",
    "posttest": "npm run lint",
    "serve": "npm start",
    "serve:dist": "node server.js --env=dist",
    "start": "parcel src/index.html",
    "test": "karma start",
    "test:watch": "karma start --autoWatch=true --singleRun=false"
  },
  "repository": "https://github.com/dggriffin/warframeLFG",
  "keywords": [],
  "author": "dggriffin",
  "engines": {
    "node": ">=12.16.1"
  },
  "devDependencies": {
    "@babel/core": "^7.13.8",
    "@babel/eslint-parser": "^7.13.8",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.13.8",
    "@babel/preset-react": "^7.12.13",
    "@parcel/optimizer-cssnano": "^2.0.0-nightly.612",
    "@parcel/optimizer-htmlnano": "^2.0.0-nightly.612",
    "@parcel/packager-css": "^2.0.0-nightly.612",
    "@parcel/packager-html": "^2.0.0-nightly.612",
    "@parcel/transformer-css": "^2.0.0-nightly.612",
    "@parcel/transformer-html": "^2.0.0-nightly.612",
    "@parcel/transformer-image": "^2.0.0-nightly.2234",
    "@parcel/transformer-postcss": "^2.0.0-nightly.612",
    "@parcel/transformer-posthtml": "^2.0.0-nightly.612",
    "@parcel/transformer-sass": "^2.0.0-nightly.612",
    "ajv": "^7.1.1",
    "bufferutil": "^4.0.3",
    "chai": "^3.2.0",
    "copyfiles": "^0.2.1",
    "eslint": "^7.21.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "fibers": "^5.0.0",
    "file-loader": "^0.8.4",
    "gh-pages": "^3.1.0",
    "glob": "^7.0.0",
    "isparta-loader": "^2.0.0",
    "karma": "^6.1.1",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^2.0.3",
    "karma-mocha": "^2.0.1",
    "karma-mocha-reporter": "^2.2.5",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.5",
    "minimist": "^1.2.5",
    "mocha": "^8.3.0",
    "node-sass": "^5.0.0",
    "null-loader": "^0.1.1",
    "open": "^7.4.2",
    "parcel": "^2.0.0-nightly.610",
    "phantomjs-prebuilt": "^2.0.0",
    "postcss": "^8.2.6",
    "postcss-loader": "^0.8.0",
    "react-hot-loader": "^4.13.0",
    "rimraf": "^2.4.3",
    "sass": "^1.32.8",
    "style-loader": "^0.13.0",
    "utf-8-validate": "^5.0.4"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "core-js": "^3.9.0",
    "json-loader": "^0.5.4",
    "normalize.css": "^3.0.3",
    "prop-types": "^15.7.2",
    "re-base": "^1.5.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-motion": "^0.5.2",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "underscore": "^1.12.0"
  },
  "contributors": [
    "Tobiah <tobiah@pm.me>"
  ],
  "babel": {
    "presets": [
      "@babel/preset-react",
      [
        "@babel/preset-env",
        {
          "modules": "auto",
          "useBuiltIns": "usage",
          "corejs": 3
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "eslintConfig": {
    "extends": "airbnb",
    "parser": "@babel/eslint-parser",
    "plugins": [
      "react"
    ],
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      },
      "babelOptions": {
        "plugins": [
          "@babel/plugin-proposal-class-properties"
        ],
        "presets": [
          "@babel/preset-react"
        ]
      }
    },
    "env": {
      "browser": true,
      "amd": true,
      "es6": true,
      "node": true,
      "mocha": true
    },
    "rules": {
      "comma-dangle": 1,
      "quotes": [
        1,
        "single"
      ],
      "no-undef": 1,
      "global-strict": 0,
      "no-extra-semi": 1,
      "no-underscore-dangle": 0,
      "no-console": 1,
      "no-unused-vars": 1,
      "no-trailing-spaces": [
        1,
        {
          "skipBlankLines": true
        }
      ],
      "no-unreachable": 1,
      "no-alert": 0,
      "react/jsx-uses-react": 1,
      "react/jsx-uses-vars": 1,
      "import/no-unresolved": 1,
      "react/no-array-index-key": 0,
      "react/static-property-placement": 0,
      "no-param-reassign": 0,
      "react/jsx-no-bind": 0,
      "react/forbid-prop-types": 0,
      "react/no-unused-prop-types": 0
    }
  },
  "license": "MIT"
}
